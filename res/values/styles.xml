    <style name="ThemeDark" parent="android:Theme.Holo">
        <item name="themeId">2</item>
        <item name="android:windowBackground">@color/holo_dark_window_background</item>
        <item name="bookmark_windoBackground">@color/darkgray</item>
        <item name="android:actionBarStyle">@style/actionBarStyleDark</item>
        <item name="ic_drawer">@drawable/holo_dark_ic_drawer</item>
        <item name="action_copy">@drawable/holo_dark_action_copy</item>
        <item name="action_cut">@drawable/holo_dark_action_cut</item>
